{
  "components/epic/sidebar-navigation-list": "{{#if children}}\n    <a class=\"navList-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n        href=\"{{url}}\"\n        title=\"{{name}}\"\n        aria-label=\"{{name}}\"\n        data-collapsible=\"sidebar-navList-{{#if id}}{{id}}{{else}}{{dashcase name}}{{/if}}\"\n    >\n        {{name}}\n        <i class=\"icon navList-action-moreIcon\" aria-hidden=\"true\">\n            <svg><use href=\"#icon-chevron-right\"></use></svg>\n        </i>\n    </a>\n    <div class=\"navList-subMenu\" id=\"sidebar-navList-{{#if id}}{{id}}{{else}}{{dashcase name}}{{/if}}\" aria-hidden=\"true\" tabindex=\"-1\">\n        <ul class=\"navList-subMenu-list\">\n            {{#if theme_settings.show_view_all}}\n                <li class=\"navList-subMenu-item\">\n                    <a class=\"navList-subMenu-action navList-action\"\n                    href=\"{{url}}\"\n                    aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n                    >\n                        {{lang 'category.view_all.name' category=name}}\n                    </a>\n                </li>\n            {{/if}}\n            {{#each children}}\n                <li class=\"navList-subMenu-item{{#if children}} navList-subMenu-item--has-subMenu{{/if}}\">\n                    {{#if children}}\n                        <a class=\"navList-subMenu-action navList-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                            href=\"{{url}}\"\n                            aria-label=\"{{name}}\"\n                        >\n                            {{name}}\n                            <span class=\"collapsible-icon-wrapper\"\n                                data-collapsible=\"sidebar-navList-{{id}}\"\n                                data-collapsible-disabled-breakpoint=\"medium\"\n                                data-collapsible-disabled-state=\"open\"\n                                data-collapsible-enabled-state=\"closed\"\n                            >\n                                <i class=\"icon navList-action-moreIcon\">\n                                    <svg><use href=\"#icon-chevron-right\" /></svg>\n                                </i>\n                            </span>\n                        </a>\n                        <ul class=\"navList-childList\" id=\"sidebar-navList-{{id}}\">\n                            {{#each children}}\n                                <li class=\"navList-childList-item\">\n                                    <a class=\"navList-childList-action navList-action{{#if is_active}} activePage{{/if}}\"\n                                        href=\"{{url}}\"\n                                        aria-label=\"{{name}}\"\n                                    >\n                                        {{name}}\n                                    </a>\n                                </li>\n                            {{/each}}\n                        </ul>\n                    {{else}}\n                        <a class=\"navList-subMenu-action navList-action{{#if is_active}} activePage{{/if}}\"\n                            href=\"{{url}}\"\n                            aria-label=\"{{name}}\"\n                        >\n                            {{name}}\n                        </a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n{{else}}\n    <a class=\"navList-action{{#if is_active}} activePage{{/if}}\"\n        href=\"{{url}}\"\n        title=\"{{name}}\"\n        aria-label=\"{{name}}\"\n    >\n        {{name}}\n    </a>\n{{/if}}"
}
