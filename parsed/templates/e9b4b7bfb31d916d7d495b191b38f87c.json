{
  "components/faceted-search/selected-facets": "<div class=\"facetedSearch-refineFilters\" role=\"region\" aria-label=\"{{lang 'search.faceted.selected.facet-label'}}\">\n    <span class=\"sidebarBlock-heading\">\n        {{lang 'search.faceted.selected.title'}}\n    </span>\n\n    {{#if items}}\n        <ul class=\"inlineList inlineList--labels refineFilters-list\">\n            {{#each items}}\n                <li>\n                    <a href=\"{{url}}\"\n                       class=\"facetLabel\"\n                       rel=\"nofollow\"\n                       data-faceted-search-facet\n                    >\n                        {{#if facet '===' 'rating'}}\n                            {{lang 'search.faceted.selected.rating-label' rating=title}}\n                        {{else}}\n                            {{{sanitize title}}}\n                        {{/if}}\n\n                        <svg class=\"icon\">\n                            <use href=\"#icon-close\" />\n                        </svg>\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n        <p class=\"refineFilters-none\">{{lang 'search.faceted.selected.no-filters'}}</p>\n    {{/if}}\n\n    {{#if remove_all_url}}\n        <a href=\"{{remove_all_url}}\" class=\"refineFilters-viewAll\" data-faceted-search-facet>\n            {{lang 'search.faceted.selected.clear-all'}}\n        </a>\n    {{/if}}\n</div>\n"
}
